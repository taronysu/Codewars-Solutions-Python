def whoseMove(lp, win):
    if lp == 'black' and win == False:
        return 'white'
    elif lp == 'white' and win == False:
        return 'black'
    elif lp == 'black' and win == True:
        return 'black'
    elif lp == 'white' and win == True:
        return 'white'
