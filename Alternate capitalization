def capitalize(s):
    first = ''
    for i,el in enumerate(s):
        if i%2==0:
            first+=el.upper()
        else:
            first+=el
    second = ''
    for i,el in enumerate(s):
        if i%2!=0:
            second+=el.upper()
        else:
            second+=el
    return [first,second]
