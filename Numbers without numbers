# NO NUMBERS IN THE CODE!!!
def numbers_without_numbers():
    s = 'ilovehorses'
    return int(str(s.index('v')) + str(s.index('o')) + str(s.index('l')) + str(s.index('h')) + str(s.index('l')) + str(s.index('e')) +str(s.index('v')) + str(s.index('o')) + str(s.index('h')))
