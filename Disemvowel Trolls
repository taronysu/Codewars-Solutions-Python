def disemvowel(s):
    vowels = 'aouieAOUIE'
    for el in s:
        if el in vowels:
            s = s.replace(el,'')
    return s
// Another Solution 

def disemvowel(s):
        return "".join(l for l in s if l.lower() not in "aouie")
