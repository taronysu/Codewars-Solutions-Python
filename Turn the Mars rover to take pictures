def turn(current, target):
    if current == 'N' and target == 'E':
        return 'right'
    elif current == 'S' and target == 'W':
        return 'right'
    elif current == 'W' and target == 'N':
        return 'right'
    elif current == 'E' and target == 'S':
        return 'right'
    return 'left'
