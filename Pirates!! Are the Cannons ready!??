def cannons_ready(gunners):
    l = list(gunners.values())
    result = all(el == l[0] for el in l)
    return 'Fire!' if result else 'Shiver me timbers!'
    
// Another Solution

def cannons_ready(gunners):
    return 'Shiver me timbers!' if 'nay' in gunners.values() else 'Fire!'
